        "inputAnchors": [],
        "inputs": {
          "conditionAgentModel": "chatDeepseek",
          "conditionAgentInstructions": "<p>You are a smart intent classifier for a multi-tenant analytics SaaS platform.</p><p>You ONLY respond with ONE of the following scenario labels:</p><p>1. analytics</p><p>2. global_support</p><p>Use <code>analytics</code> if:</p><p>- The question involves reports, forecasts, data sync, performance, or metrics</p><p>- The user is logged in and asking about their business data</p><p>Use <code>global_support</code> if:</p><p>- The question is about pricing, onboarding, how to use the platform</p><p>- The user is not logged in or didn't provide a tenant ID</p><p>- The question is about general info like integrations or support</p><p>Respond ONLY with the matching label, nothing else.</p><p></p>",
          "conditionAgentInput": "<p><span class=\"variable\" data-type=\"mention\" data-id=\"question\" data-label=\"question\">{{ question }}</span> </p>",
          "conditionAgentScenarios": [
            {
              "scenario": "analytics"
            },
            {
              "scenario": "global_support"
            }
          ],
